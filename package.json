{
  "name": "CryptoTracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "tsc",
    "compile": "tsc",
    "clean-dist": "rimraf dist",
    "build": "yarn clean-dist && yarn compile --",
    "watch": "yarn build -- -w",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "yarn build && concurrently -r 'yarn watch' 'node node_modules/react-native/local-cli/cli.js run-ios'",
    "test": "jest"
  },
  "dependencies": {
    "appcenter": "^1.1.0",
    "appcenter-analytics": "^1.1.0",
    "appcenter-crashes": "^1.1.0",
    "react": "16.0.0",
    "react-native": "0.51.0",
    "react-native-vector-icons": "^4.4.3",
    "react-navigation": "^1.0.0-beta.23",
    "react-timer-mixin": "^0.13.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "devDependencies": {
    "@types/react": "^16.0.33",
    "@types/react-native": "^0.51.8",
    "@types/react-navigation": "^1.0.26",
    "concurrently": "^3.5.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
